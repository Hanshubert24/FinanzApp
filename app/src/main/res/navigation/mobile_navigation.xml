<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.finanzapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home"  />

    <activity
        android:id="@+id/nav_financeBook"
        android:name="com.example.finanzapp.ui.financebook.FinanceBookOverview"
        android:label="@string/menu_financebook"
        tools:layout="@layout/activity_finance_book_overview" />

    <activity
        android:id="@+id/nav_assets"
        android:name="com.example.finanzapp.ui.Assets.AssetsOverview"
        android:label="@string/menu_assets"
        tools:layout="@layout/activity_assets_overview" />
    <activity
        android:id="@+id/nav_contracts"
        android:name="com.example.finanzapp.ui.Contracts.ContractsOverview"
        android:label="@string/menu_contracts"
        tools:layout="@layout/activity_contracts_overview" />

    <activity
        android:id="@+id/nav_logout"
        android:name="com.example.finanzapp.MainActivity"
        android:label="@string/menu_logout"
        tools:layout="@layout/activity_logout" />
    <activity
        android:id="@+id/nav_logout2"
        android:name="com.example.finanzapp.MainActivity"
